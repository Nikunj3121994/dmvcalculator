@model object

<div ng-controller="dmvController">

    <form id="dmvform">
        <input type="text" ng-model="inputData" />
    </form>

    <p>{{inputData}}</p>

    <div>{{first}}</div>

    <ul>
        <li ng-repeat="item in items">
            {{item.description}}
        </li>
    </ul>
</div>
